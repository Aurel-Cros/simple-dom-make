{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "sourceMap": false,
    "emitDeclarationOnly": false,
    "removeComments": true
  }
}